	<rul:Rule Name="ErcSetStatisticalCodes" Type="none" DataDefGroup="CommercialPropertyOrdinanceOrLawCoverage">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:Exist AtInputDataDef="ancestor::MasterCFCW/Policy" />
				</rul:Test>
				<rul:Then>
					<rul:Sequence>
						<rul:If>
							<rul:Test>
								<rul:Equal>
									<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CovType" Order="DataDefInputParamConstant" />
									<rul:Constant Type="string">Coverage A Only</rul:Constant>
								</rul:Equal>
							</rul:Test>
							<rul:Then>
								<rul:Sequence>
									<rul:RunRule Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovAOnly" FileName="CommercialPropertyOrdinanceOrLawCoverageRules" Rule="LookupOrdinanceOrLawCoverageIndicatorCovAOnly" ClearCache="true" />
								</rul:Sequence>
							</rul:Then>
							<rul:Else>
								<rul:Sequence>
									<rul:Constant Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovAOnly"></rul:Constant>
								</rul:Sequence>
							</rul:Else>
						</rul:If>
						<rul:If>
							<rul:Test>
								<rul:Equal>
									<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CovType" Order="DataDefInputParamConstant" />
									<rul:Constant Type="string">Coverage C Only</rul:Constant>
								</rul:Equal>
							</rul:Test>
							<rul:Then>
								<rul:Sequence>
									<rul:RunRule Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovCOnly" FileName="CommercialPropertyOrdinanceOrLawCoverageRules" Rule="LookupOrdinanceOrLawCoverageIndicatorCovCOnly" ClearCache="true" />
								</rul:Sequence>
							</rul:Then>
							<rul:Else>
								<rul:Sequence>
									<rul:Constant Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovCOnly"></rul:Constant>
								</rul:Sequence>
							</rul:Else>
						</rul:If>
						<rul:If>
							<rul:Test>
								<rul:Equal>
									<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CovType" Order="DataDefInputParamConstant" />
									<rul:Constant Type="string">Coverages A and B</rul:Constant>
								</rul:Equal>
							</rul:Test>
							<rul:Then>
								<rul:Sequence>
									<rul:RunRule Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovAAndCovB" FileName="CommercialPropertyOrdinanceOrLawCoverageRules" Rule="LookupOrdinanceOrLawCoverageIndicatorCovAAndCovB" ClearCache="true" />
								</rul:Sequence>
							</rul:Then>
							<rul:Else>
								<rul:Sequence>
									<rul:Constant Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovAAndCovB"></rul:Constant>
								</rul:Sequence>
							</rul:Else>
						</rul:If>
						<rul:If>
							<rul:Test>
								<rul:Equal>
									<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CovType" Order="DataDefInputParamConstant" />
									<rul:Constant Type="string">Coverages A and B &amp; C Separate</rul:Constant>
								</rul:Equal>
							</rul:Test>
							<rul:Then>
								<rul:Sequence>
									<rul:RunRule Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovACovBAndCovCSeparateCovB" FileName="CommercialPropertyOrdinanceOrLawCoverageRules" Rule="LookupOrdinanceOrLawCoverageIndicatorCovACovBAndCovCSeparateCovB" ClearCache="true" />
								</rul:Sequence>
							</rul:Then>
							<rul:Else>
								<rul:Sequence>
									<rul:Constant Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovACovBAndCovCSeparateCovB"></rul:Constant>
								</rul:Sequence>
							</rul:Else>
						</rul:If>
						<rul:If>
							<rul:Test>
								<rul:Equal>
									<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CovType" Order="DataDefInputParamConstant" />
									<rul:Constant Type="string">Coverages A and B &amp; C Separate</rul:Constant>
								</rul:Equal>
							</rul:Test>
							<rul:Then>
								<rul:Sequence>
									<rul:RunRule Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovACovBAndCovCSeparateCovC" FileName="CommercialPropertyOrdinanceOrLawCoverageRules" Rule="LookupOrdinanceOrLawCoverageIndicatorCovACovBAndCovCSeparateCovC" ClearCache="true" />
								</rul:Sequence>
							</rul:Then>
							<rul:Else>
								<rul:Sequence>
									<rul:Constant Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovACovBAndCovCSeparateCovC"></rul:Constant>
								</rul:Sequence>
							</rul:Else>
						</rul:If>
						<rul:If>
							<rul:Test>
								<rul:Equal>
									<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CovType" Order="DataDefInputParamConstant" />
									<rul:Constant Type="string">Coverages A and B &amp; C Combined</rul:Constant>
								</rul:Equal>
							</rul:Test>
							<rul:Then>
								<rul:Sequence>
									<rul:RunRule Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovACovBAndCovCCombined" FileName="CommercialPropertyOrdinanceOrLawCoverageRules" Rule="LookupOrdinanceOrLawCoverageIndicatorCovACovBAndCovCCombined" ClearCache="true" />
								</rul:Sequence>
							</rul:Then>
							<rul:Else>
								<rul:Sequence>
									<rul:Constant Type="string" ToDataDef="@OrdinanceOrLawCoverageIndicatorCovACovBAndCovCCombined"></rul:Constant>
								</rul:Sequence>
							</rul:Else>
						</rul:If>
						<rul:WithArgs>
							<rul:Arg Type="integer" Param="calcExposureStatAmount">
								<rul:FirstNonNull Type="integer">
									<rul:If>
										<rul:Test>
											<rul:NotEqual>
												<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CovType" Order="DataDefInputParamConstant" />
												<rul:Constant Type="string">Coverage C Only</rul:Constant>
											</rul:NotEqual>
										</rul:Test>
										<rul:Then>
											<rul:Convert Type="integer">
												<rul:Truncate>
													<rul:Divide>
														<rul:Sum>
															<rul:Value Type="integer" FromDataDef="@CovBDemolitionLimit" />
															<rul:Value Type="integer" FromDataDef="@CovCIncreasedConstructionLimit" />
															<rul:Value Type="integer" FromDataDef="@CombinedBAndCDemolitionAndIncrdConstrctnLimit" />
															<rul:Value Type="integer" FromDataDef="ancestor::CommercialPropertyStructure/@LimitToUse" />
														</rul:Sum>
														<rul:Constant Type="integer">1000</rul:Constant>
													</rul:Divide>
												</rul:Truncate>
											</rul:Convert>
										</rul:Then>
										<rul:Else>
											<rul:Convert Type="integer">
												<rul:Truncate>
													<rul:Divide>
														<rul:Sum>
															<rul:Value Type="integer" FromDataDef="@CovBDemolitionLimit" />
															<rul:Value Type="integer" FromDataDef="@CovCIncreasedConstructionLimit" />
															<rul:Value Type="integer" FromDataDef="@CombinedBAndCDemolitionAndIncrdConstrctnLimit" />
														</rul:Sum>
														<rul:Constant Type="integer">1000</rul:Constant>
													</rul:Divide>
												</rul:Truncate>
											</rul:Convert>
										</rul:Else>
									</rul:If>
									<rul:Constant Type="integer">0</rul:Constant>
								</rul:FirstNonNull>
							</rul:Arg>
							<rul:Value Type="integer" ToDataDef="@ExposureStatAmount" FromParam="calcExposureStatAmount" />
						</rul:WithArgs>
					</rul:Sequence>
				</rul:Then>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
