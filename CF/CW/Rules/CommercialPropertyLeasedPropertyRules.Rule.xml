<rul:Rules xmlns:rul="http://www.verisk.com/iso/erc/Rule">
	<rul:Rule Name="InitializeRuleSet" DataDefGroup="CommercialPropertyLeasedProperty">
		<rul:Sequence>
			<rul:ForEach AtDataDef="CommercialPropertyLeasedPropertyDetailTable/CommercialPropertyLeasedPropertyDetail">
				<rul:RunRule Type="none" FileName="CommercialPropertyLeasedPropertyDetailRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:ForEach>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="CalculateTotalPremium" Type="decimal" DataDefGroup="CommercialPropertyLeasedProperty">
		<rul:Sum>
			<rul:ForEach AtDataDef="CommercialPropertyLeasedPropertyDetailTable/CommercialPropertyLeasedPropertyDetail">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyLeasedPropertyDetailRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:ForEach>
			<rul:If>
				<rul:Test>
					<rul:And>
						<rul:Equal>
							<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@PremiumIndicator" Order="DataDefInputParamConstant" />
							<rul:Constant Type="integer">1</rul:Constant>
						</rul:Equal>
						<rul:NotEqual>
							<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
							<rul:Constant Type="decimal">0</rul:Constant>
						</rul:NotEqual>
					</rul:And>
				</rul:Test>
				<rul:Then>
					<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
				</rul:Then>
				<rul:Else>
					<rul:Constant Type="decimal">0</rul:Constant>
				</rul:Else>
			</rul:If>
		</rul:Sum>
	</rul:Rule>
	<rul:Rule Name="ErcProcess" Type="none" DataDefGroup="CommercialPropertyLeasedProperty">
		<rul:Sequence>
			<rul:ForEach AtDataDef="CommercialPropertyLeasedPropertyDetailTable/CommercialPropertyLeasedPropertyDetail">
				<rul:RunRule Type="none" FileName="CommercialPropertyLeasedPropertyDetailRules" Rule="ErcProcess" ClearCache="true" />
			</rul:ForEach>
		</rul:Sequence>
	</rul:Rule>
</rul:Rules>