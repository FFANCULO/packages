			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetBGIINonOverheadRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetBGIIOverheadRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetBGIIRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetBroadNonOverheadRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetBroadOverheadRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetBroadRate" ClearCache="true" />
