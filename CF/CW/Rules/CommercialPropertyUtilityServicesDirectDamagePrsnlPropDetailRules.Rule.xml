		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetLimitToUse" Type="none" DataDefGroup="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetail">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:GreaterThan>
						<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@Limit" Order="DataDefInputParamConstant" />
						<rul:Constant Type="integer">0</rul:Constant>
					</rul:GreaterThan>
				</rul:Test>
				<rul:Then>
					<rul:Copy Type="integer" ToDataDef="@LimitToUse" FromDataDef="@Limit" />
				</rul:Then>
				<rul:Else>
					<rul:Copy Type="integer" ToDataDef="@LimitToUse" FromDataDef="ancestor::CommercialPropertyPersonalProperty/@LimitToUse" />
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetEQLimitToUse" Type="none" DataDefGroup="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetail">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:And>
						<rul:Or>
							<rul:Exist AtInputDataDef="ancestor::CommercialPropertyPersonalProperty/CommercialPropertyEarthquakeAndVolcanicEruptionEndorsementSubLimitFormPrsnlPropTable/CommercialPropertyEarthquakeAndVolcanicEruptionEndorsementSubLimitFormPrsnlProp" />
							<rul:Exist AtInputDataDef="ancestor::CommercialPropertyPersonalProperty/CommercialPropertyEarthquakeAndVolcanicEruptionCoverageSubLimitFormWithFlatDollarDeductiblePrsnlPropTable/CommercialPropertyEarthquakeAndVolcanicEruptionCoverageSubLimitFormWithFlatDollarDeductiblePrsnlProp" />
						</rul:Or>
						<rul:LessThan>
							<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="ancestor::CommercialPropertyPersonalProperty/@EQLimitToUse" Order="DataDefInputParamConstant" />
							<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@LimitToUse" Order="DataDefInputParamConstant" />
						</rul:LessThan>
					</rul:And>
				</rul:Test>
				<rul:Then>
					<rul:Copy Type="integer" ToDataDef="@EQLimitToUse" FromDataDef="ancestor::CommercialPropertyPersonalProperty/@EQLimitToUse" />
				</rul:Then>
				<rul:Else>
					<rul:Copy Type="integer" ToDataDef="@EQLimitToUse" FromDataDef="@LimitToUse" />
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetBGIRate" Type="none" DataDefGroup="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetail">
