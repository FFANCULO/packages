<rul:Rules xmlns:rul="http://www.verisk.com/iso/erc/Rule">
	<rul:Rule Name="InitializeRuleSet" DataDefGroup="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetail">
		<rul:Sequence>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupICoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupICoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupIICoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupIICoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBroadCoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBroadCoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailEarthquakeCoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailEarthquakeCoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailSpecialCoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailSpecialCoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="CalculateTotalPremium" Type="decimal" DataDefGroup="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetail">
		<rul:Sum>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupICoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupICoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupIICoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBasicGroupIICoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBroadCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailBroadCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailEarthquakeCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailEarthquakeCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailSpecialCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailSpecialCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:If>
				<rul:Test>
					<rul:And>
						<rul:Equal>
							<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@PremiumIndicator" Order="DataDefInputParamConstant" />
							<rul:Constant Type="integer">1</rul:Constant>
						</rul:Equal>
						<rul:NotEqual>
							<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
							<rul:Constant Type="decimal">0</rul:Constant>
						</rul:NotEqual>
					</rul:And>
				</rul:Test>
				<rul:Then>
					<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
				</rul:Then>
				<rul:Else>
					<rul:Constant Type="decimal">0</rul:Constant>
				</rul:Else>
			</rul:If>
		</rul:Sum>
	</rul:Rule>
	<rul:Rule Name="ErcSetRatesAndFactors" Type="none" DataDefGroup="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetail">
		<rul:Sequence>
			<rul:Copy Type="string" ToDataDef="@PolicyType" FromDataDef="ancestor::CommercialProperty/@PolicyType" />
			<rul:Copy Type="string" ToDataDef="@CauseOfLossValueFromRiskLevel" FromDataDef="ancestor::CommercialPropertyPersonalProperty/@CauseOfLoss" />
