			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetSpecialRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetEQNonOverheadRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="SetEQOverheadRate" ClearCache="true" />
