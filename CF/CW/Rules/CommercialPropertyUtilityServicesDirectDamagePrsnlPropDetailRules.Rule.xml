	<rul:Rule Name="SetBGINonOverheadRate" Type="none" DataDefGroup="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetail">
		<rul:Sequence>
			<!--Logic for BasicGroupINonOverheadBaseRate moved to be inline where used.-->
			<rul:WithArgs>
				<rul:Arg Type="decimal" Param="calcBasicGroupINonOverheadRate">
					<rul:FirstNonNull Type="decimal">
						<rul:Product DecimalPlaces="3">
							<rul:If>
								<rul:Test>
									<rul:NotEqual>
										<rul:FirstValue Type="string" FromConstant="" FromDataDef="@UtilityType" Order="DataDefInputParamConstant" />
										<rul:Constant Type="string"></rul:Constant>
									</rul:NotEqual>
								</rul:Test>
								<rul:Then>
									<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamagePrsnlPropDetailRules" Rule="LookupUtilitySrvcDirectDamageWaterPowerAndCommunicationSupply" ClearCache="true">
										<rul:Arg Type="string" Param="utilitySrvcCauseOfLoss">
											<rul:Constant Type="string">GroupI</rul:Constant>
										</rul:Arg>
									</rul:RunRule>
								</rul:Then>
								<rul:Else>
									<rul:Constant Type="decimal">0</rul:Constant>
								</rul:Else>
							</rul:If>
							<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="ancestor::CommercialProperty/@LossCostMultiplier" Order="DataDefInputParamConstant" />
						</rul:Product>
						<rul:Constant Type="decimal">0</rul:Constant>
					</rul:FirstNonNull>
				</rul:Arg>
				<rul:Value Type="decimal" ToDataDef="@BasicGroupINonOverheadRate" FromParam="calcBasicGroupINonOverheadRate" />
			</rul:WithArgs>
		</rul:Sequence>
	</rul:Rule>
