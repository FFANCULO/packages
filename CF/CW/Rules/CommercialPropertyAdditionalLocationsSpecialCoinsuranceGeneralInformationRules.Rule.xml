	</rul:Rule>
	<rul:Rule Name="SetCoinsuranceFactor" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:Equal>
						<rul:FirstValue Type="string" FromConstant="" FromDataDef="@Coinsurance" Order="DataDefInputParamConstant" />
						<rul:Constant Type="string">90%</rul:Constant>
					</rul:Equal>
				</rul:Test>
				<rul:Then>
					<rul:Sequence>
						<rul:RunRule Type="decimal" ToDataDef="@CoinsuranceFactor" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="LookupAddlLocSpclCoinsFactor" ClearCache="true" />
					</rul:Sequence>
				</rul:Then>
				<rul:Else>
					<rul:Sequence>
						<rul:Constant Type="decimal" ToDataDef="@CoinsuranceFactor">1.0</rul:Constant>
					</rul:Sequence>
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetAddlLocsSpclCoinsPrsnlPropCounter" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:WithArgs>
				<rul:Arg Type="integer" Param="AddlLocsSpclCoinsPrsnlPropCounterIncrementParam">
					<rul:Sum>
						<rul:ForEach AtInputDataDef="ancestor::CommercialProperty/CommercialPropertyLocationTable/CommercialPropertyLocation">
							<rul:ForEach AtInputDataDef="CommercialPropertyStructureTable/CommercialPropertyStructure">
								<rul:ForEach AtInputDataDef="CommercialPropertyOccupClassTable/CommercialPropertyOccupClass">
									<rul:If>
										<rul:Test>
											<rul:GreaterThanOrEqual>
												<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@AddlLocsSpclCoinsCounter" Order="DataDefInputParamConstant" />
												<rul:Constant Type="integer">1</rul:Constant>
											</rul:GreaterThanOrEqual>
										</rul:Test>
										<rul:Then>
											<rul:Constant Type="integer">1</rul:Constant>
										</rul:Then>
									</rul:If>
								</rul:ForEach>
							</rul:ForEach>
						</rul:ForEach>
						<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@AddlLocsSpclCoinsPrsnlPropCounter" Order="DataDefInputParamConstant" />
					</rul:Sum>
				</rul:Arg>
				<rul:Value Type="integer" ToDataDef="@AddlLocsSpclCoinsPrsnlPropCounter" FromParam="AddlLocsSpclCoinsPrsnlPropCounterIncrementParam" />
			</rul:WithArgs>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetEQSubLimitCounter" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:WithArgs>
				<rul:Arg Type="integer" Param="EQSubLimitCounterIncrementParam">
					<rul:Sum>
						<rul:ForEach AtInputDataDef="ancestor::CommercialProperty/CommercialPropertyLocationTable/CommercialPropertyLocation">
							<rul:ForEach AtInputDataDef="CommercialPropertyStructureTable/CommercialPropertyStructure">
								<rul:ForEach AtInputDataDef="CommercialPropertyOccupClassTable/CommercialPropertyOccupClass">
									<rul:If>
										<rul:Test>
											<rul:GreaterThanOrEqual>
												<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@EQSubLimitCounter" Order="DataDefInputParamConstant" />
												<rul:Constant Type="integer">1</rul:Constant>
											</rul:GreaterThanOrEqual>
										</rul:Test>
										<rul:Then>
											<rul:Constant Type="integer">1</rul:Constant>
										</rul:Then>
									</rul:If>
								</rul:ForEach>
							</rul:ForEach>
						</rul:ForEach>
						<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@EQSubLimitCounter" Order="DataDefInputParamConstant" />
					</rul:Sum>
				</rul:Arg>
				<rul:Value Type="integer" ToDataDef="@EQSubLimitCounter" FromParam="EQSubLimitCounterIncrementParam" />
			</rul:WithArgs>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetEQCovCounter" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:WithArgs>
				<rul:Arg Type="integer" Param="EQCovCounterIncrementParam">
					<rul:Sum>
						<rul:ForEach AtInputDataDef="ancestor::CommercialProperty/CommercialPropertyLocationTable/CommercialPropertyLocation">
							<rul:ForEach AtInputDataDef="CommercialPropertyStructureTable/CommercialPropertyStructure">
								<rul:ForEach AtInputDataDef="CommercialPropertyOccupClassTable/CommercialPropertyOccupClass">
									<rul:If>
										<rul:Test>
											<rul:GreaterThanOrEqual>
												<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@EQCovCounter" Order="DataDefInputParamConstant" />
												<rul:Constant Type="integer">1</rul:Constant>
											</rul:GreaterThanOrEqual>
										</rul:Test>
										<rul:Then>
											<rul:Constant Type="integer">1</rul:Constant>
										</rul:Then>
									</rul:If>
								</rul:ForEach>
							</rul:ForEach>
						</rul:ForEach>
						<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@EQCovCounter" Order="DataDefInputParamConstant" />
					</rul:Sum>
				</rul:Arg>
				<rul:Value Type="integer" ToDataDef="@EQCovCounter" FromParam="EQCovCounterIncrementParam" />
			</rul:WithArgs>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetTotalFullValueAmount" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:WithArgs>
				<rul:Arg Type="integer" Param="calcTotalFullValueAmount">
					<rul:FirstNonNull Type="integer">
						<rul:Sum>
							<rul:ForEach AtDataDef="ancestor::CommercialProperty/CommercialPropertyLocationTable/CommercialPropertyLocation">
								<rul:ForEach AtDataDef="CommercialPropertyStructureTable/CommercialPropertyStructure">
									<rul:ForEach AtDataDef="CommercialPropertyOccupClassTable/CommercialPropertyOccupClass">
										<rul:ForEach AtDataDef="CommercialPropertyPersonalPropertyTable/CommercialPropertyPersonalProperty">
											<rul:If>
												<rul:Test>
													<rul:And>
														<rul:Exist AtInputDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceProvisionsTable/CommercialPropertyAdditionalLocationsSpecialCoinsuranceProvisions" />
														<rul:IsNotNull>
															<rul:Value Type="integer" FromDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceProvisionsTable/CommercialPropertyAdditionalLocationsSpecialCoinsuranceProvisions[1]/@FullValue" />
														</rul:IsNotNull>
													</rul:And>
												</rul:Test>
												<rul:Then>
													<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceProvisionsTable/CommercialPropertyAdditionalLocationsSpecialCoinsuranceProvisions[1]/@FullValue" Order="DataDefInputParamConstant" />
												</rul:Then>
											</rul:If>
										</rul:ForEach>
									</rul:ForEach>
								</rul:ForEach>
							</rul:ForEach>
						</rul:Sum>
						<rul:Constant Type="integer">0</rul:Constant>
					</rul:FirstNonNull>
				</rul:Arg>
				<rul:Value Type="integer" ToDataDef="@TotalFullValueAmount" FromParam="calcTotalFullValueAmount" />
			</rul:WithArgs>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetTotalFullValueTimesFinalBasicGroupIRate" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
