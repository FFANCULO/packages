<rul:Rules xmlns:rul="http://www.verisk.com/iso/erc/Rule">
	<rul:Rule Name="InitializeRuleSet" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupICoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupICoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupIICoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupIICoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBroadCoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBroadCoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationEarthquakeCoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationEarthquakeCoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationSpecialCoverage">
				<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationSpecialCoverageRules" Rule="InitializeRuleSet" ClearCache="true" />
			</rul:Locate>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="CalculateTotalPremium" Type="decimal" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sum>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupICoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupICoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupIICoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBasicGroupIICoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBroadCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationBroadCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationEarthquakeCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationEarthquakeCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationSpecialCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationSpecialCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:If>
				<rul:Test>
					<rul:And>
						<rul:Equal>
							<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@PremiumIndicator" Order="DataDefInputParamConstant" />
							<rul:Constant Type="integer">1</rul:Constant>
						</rul:Equal>
						<rul:NotEqual>
							<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
							<rul:Constant Type="decimal">0</rul:Constant>
						</rul:NotEqual>
					</rul:And>
				</rul:Test>
				<rul:Then>
					<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
				</rul:Then>
				<rul:Else>
					<rul:Constant Type="decimal">0</rul:Constant>
				</rul:Else>
			</rul:If>
		</rul:Sum>
	</rul:Rule>
	<rul:Rule Name="ErcSetRatesAndFactors" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:Constant Type="integer" ToDataDef="@AddlLocsSpclCoinsPrsnlPropCounter">0</rul:Constant>
			<rul:Constant Type="integer" ToDataDef="@EQCovCounter">0</rul:Constant>
			<rul:Constant Type="integer" ToDataDef="@EQSubLimitCounter">0</rul:Constant>
			<rul:Constant Type="integer" ToDataDef="@TotalIncLocsLimit">0</rul:Constant>
			<rul:Constant Type="integer" ToDataDef="@TotalFullValueWOMiscLocsAmount">0</rul:Constant>
			<rul:Copy Type="string" ToDataDef="@PolicyType" FromDataDef="ancestor::CommercialProperty/@PolicyType" />
			<rul:RunRule Type="decimal" ToDataDef="@BasicGroupIRateIncLocs" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="LookupValRptgIncLocsRate" ClearCache="true">
				<rul:Arg Type="string" Param="causeOfLoss">
					<rul:Constant Type="string">Basic</rul:Constant>
				</rul:Arg>
			</rul:RunRule>
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetBroadRateIncLocs" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetSpecialRateLocs" ClearCache="true" />
			<rul:RunRule Type="decimal" ToDataDef="@EQRateLocs" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="LookupValRptgIncLocsRate" ClearCache="true">
				<rul:Arg Type="string" Param="causeOfLoss">
					<rul:Constant Type="string">Earthquake</rul:Constant>
				</rul:Arg>
			</rul:RunRule>
			<rul:Sum ToDataDef="@TotalIncLocsLimit">
				<rul:Value Type="integer" FromDataDef="@YourBusnPrsnlPropInc" />
				<rul:Value Type="integer" FromDataDef="@StockOnlyInc" />
				<rul:Value Type="integer" FromDataDef="@PrsnlPropOthersInc" />
			</rul:Sum>
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetCoinsuranceFactor" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetAddlLocsSpclCoinsPrsnlPropCounter" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetEQSubLimitCounter" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetEQCovCounter" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetTotalFullValueAmount" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetTotalFullValueTimesFinalBasicGroupIRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetTotalFullValueTimesFinalBasicGroupIIRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetTotalFullValueTimesFinalBroadRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetTotalFullValueTimesFinalSpecialRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetTotalFullValueTimesFinalEQRate" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetTotalFullValueForMLARCalc" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetFinalBasicGroupIRateTimesValue" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetBasicGroupIMLAR" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetFinalBasicGroupIIRateTimesValue" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetBasicGroupIIMLAR" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetFinalBroadRateTimesValue" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetBroadMLAR" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetFinalSpecialRateTimesValue" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetSpecialMLAR" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetFinalEQRateTimesValue" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="SetEQMLAR" ClearCache="true" />
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetBroadRateIncLocs" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:Equal>
						<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CauseOfLoss" Order="DataDefInputParamConstant" />
						<rul:Constant Type="string">Broad</rul:Constant>
					</rul:Equal>
				</rul:Test>
				<rul:Then>
					<rul:Sequence>
						<rul:RunRule Type="decimal" ToDataDef="@BroadRateIncLocs" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="LookupValRptgIncLocsRate" ClearCache="true">
							<rul:Arg Type="string" Param="causeOfLoss">
								<rul:Constant Type="string">Broad</rul:Constant>
							</rul:Arg>
						</rul:RunRule>
					</rul:Sequence>
				</rul:Then>
				<rul:Else>
					<rul:Sequence>
						<rul:Constant Type="decimal" ToDataDef="@BroadRateIncLocs">0.0</rul:Constant>
					</rul:Sequence>
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetSpecialRateLocs" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:Equal>
						<rul:FirstValue Type="string" FromConstant="" FromDataDef="@CauseOfLoss" Order="DataDefInputParamConstant" />
