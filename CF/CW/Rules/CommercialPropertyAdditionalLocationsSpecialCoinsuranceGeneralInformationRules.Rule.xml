			<!--Logic for EQCalc moved to be inline where used.-->
			<rul:Constant Type="integer" ToDataDef="@TotalFullValueTimesFinalEQRate">0</rul:Constant>
			<!--Logic for EQCalc moved to be inline where used.-->
			<rul:RunRule Type="integer" ToDataDef="@TotalFullValueTimesFinalEQRate" FileName="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformationRules" Rule="EQCalc" ClearCache="true" />
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetTotalFullValueForMLARCalc" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:WithArgs>
				<rul:Arg Type="integer" Param="calcTotalFullValueForMLARCalc">
					<rul:FirstNonNull Type="integer">
						<rul:Sum>
							<rul:Value Type="integer" FromDataDef="@TotalIncLocsLimit" />
							<rul:Value Type="integer" FromDataDef="@TotalFullValueAmount" />
						</rul:Sum>
						<rul:Constant Type="integer">0</rul:Constant>
					</rul:FirstNonNull>
				</rul:Arg>
				<rul:Value Type="integer" ToDataDef="@TotalFullValueForMLARCalc" FromParam="calcTotalFullValueForMLARCalc" />
			</rul:WithArgs>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetFinalBasicGroupIRateTimesValue" Type="none" DataDefGroup="CommercialPropertyAdditionalLocationsSpecialCoinsuranceGeneralInformation">
		<rul:Sequence>
			<rul:WithArgs>
				<rul:Arg Type="decimal" Param="calcFinalBasicGroupIRateTimesValue">
					<rul:FirstNonNull Type="decimal">
						<rul:Sum>
							<rul:Product DecimalPlaces="0">
								<rul:Product DecimalPlaces="3">
									<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@BasicGroupIRateIncLocs" Order="DataDefInputParamConstant" />
									<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="ancestor::CommercialProperty/@LossCostMultiplier" Order="DataDefInputParamConstant" />
								</rul:Product>
								<rul:Divide>
									<rul:Value Type="integer" FromDataDef="@TotalIncLocsLimit" />
									<rul:Constant Type="decimal">100</rul:Constant>
								</rul:Divide>
							</rul:Product>
							<rul:Value Type="integer" FromDataDef="@TotalFullValueTimesFinalBasicGroupIRate" />
						</rul:Sum>
						<rul:Constant Type="decimal">0</rul:Constant>
