			</rul:Locate>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="CalculateTotalPremium" Type="decimal" DataDefGroup="CommercialPropertyUtilityServicesDirectDamageBldgDetail">
		<rul:Sum>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamageBldgDetailBasicGroupICoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamageBldgDetailBasicGroupICoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamageBldgDetailBasicGroupIICoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamageBldgDetailBasicGroupIICoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamageBldgDetailBroadCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamageBldgDetailBroadCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamageBldgDetailEarthquakeCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamageBldgDetailEarthquakeCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
			<rul:Locate AtDataDef="CommercialPropertyUtilityServicesDirectDamageBldgDetailSpecialCoverage">
				<rul:RunRule Type="decimal" FileName="CommercialPropertyUtilityServicesDirectDamageBldgDetailSpecialCoverageRules" Rule="CalculateTotalPremium" ClearCache="true" />
			</rul:Locate>
