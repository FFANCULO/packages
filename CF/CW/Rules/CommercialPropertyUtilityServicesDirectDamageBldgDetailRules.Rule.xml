						</rul:Or>
						<rul:LessThan>
							<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="../../../../@EQLimitToUse" Order="DataDefInputParamConstant" />
							<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@LimitToUse" Order="DataDefInputParamConstant" />
						</rul:LessThan>
					</rul:And>
				</rul:Test>
				<rul:Then>
					<rul:Copy Type="integer" ToDataDef="@EQLimitToUse" FromDataDef="../../../../@EQLimitToUse" />
				</rul:Then>
				<rul:Else>
					<rul:Copy Type="integer" ToDataDef="@EQLimitToUse" FromDataDef="@LimitToUse" />
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
