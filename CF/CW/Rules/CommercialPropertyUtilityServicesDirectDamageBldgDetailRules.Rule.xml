					<rul:And>
						<rul:Or>
							<rul:Exist AtInputDataDef="ancestor::CommercialPropertyStructure/CommercialPropertyEarthquakeAndVolcanicEruptionEndorsementSubLimitFormTable/CommercialPropertyEarthquakeAndVolcanicEruptionEndorsementSubLimitForm" />
