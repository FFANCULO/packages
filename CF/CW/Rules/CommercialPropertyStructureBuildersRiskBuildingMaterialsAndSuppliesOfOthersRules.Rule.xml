<rul:Rules xmlns:rul="http://www.verisk.com/iso/erc/Rule">
	<rul:Rule Name="InitializeRuleSet" DataDefGroup="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthers">
		<rul:Sequence>
			<rul:RunRule Type="none" FileName="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthersRules" Rule="SetCoverageOnPolicyIndicator" ClearCache="true" />
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="CalculateTotalPremium" Type="decimal" DataDefGroup="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthers">
		<rul:If>
			<rul:Test>
				<rul:And>
					<rul:Equal>
						<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@PremiumIndicator" Order="DataDefInputParamConstant" />
						<rul:Constant Type="integer">1</rul:Constant>
					</rul:Equal>
					<rul:NotEqual>
						<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
						<rul:Constant Type="decimal">0</rul:Constant>
					</rul:NotEqual>
				</rul:And>
			</rul:Test>
			<rul:Then>
				<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
			</rul:Then>
			<rul:Else>
				<rul:Constant Type="decimal">0</rul:Constant>
			</rul:Else>
		</rul:If>
	</rul:Rule>
	<rul:Rule Name="SetCoverageOnPolicyIndicator" Type="none" DataDefGroup="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthers">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:Equal>
						<rul:FirstValue Type="string" FromConstant="" FromDataDef="@Indicator" Order="DataDefInputParamConstant" />
						<rul:Constant Type="string">Yes</rul:Constant>
					</rul:Equal>
				</rul:Test>
				<rul:Then>
					<rul:Sequence>
						<rul:Constant Type="integer" ToDataDef="@CoverageOnPolicyIndicator">1</rul:Constant>
					</rul:Sequence>
				</rul:Then>
				<rul:Else>
					<rul:Sequence>
						<rul:Constant Type="integer" ToDataDef="@CoverageOnPolicyIndicator">0</rul:Constant>
					</rul:Sequence>
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="ErcRate" Type="none" DataDefGroup="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthers">
		<rul:Sequence>
			<rul:RunRule Type="none" FileName="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthersRules" Rule="SetPremium" ClearCache="true" />
			<rul:RunRule Type="none" FileName="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthersRules" Rule="SetPremiumIndicator" ClearCache="true" />
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetPremium" Type="none" DataDefGroup="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthers">
		<rul:Sequence>
			<rul:WithArgs>
				<rul:Arg Type="decimal" Param="calcPremium">
					<rul:FirstNonNull Type="decimal">
						<rul:Product DecimalPlaces="0">
							<rul:Product DecimalPlaces="0">
								<rul:Product DecimalPlaces="0">
									<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@ManualPremium" Order="DataDefInputParamConstant" />
									<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="../../../../../@PackageModFactor" Order="DataDefInputParamConstant" />
								</rul:Product>
								<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="../@MultiPremiumAndDispersionCreditFactor" Order="DataDefInputParamConstant" />
							</rul:Product>
							<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="../../../../../@IRPMFactor" Order="DataDefInputParamConstant" />
						</rul:Product>
						<rul:Constant Type="decimal">0</rul:Constant>
					</rul:FirstNonNull>
				</rul:Arg>
				<rul:Value Type="decimal" ToDataDef="@Premium" FromParam="calcPremium" />
			</rul:WithArgs>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="SetPremiumIndicator" Type="none" DataDefGroup="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthers">
		<rul:Sequence>
			<rul:If>
				<rul:Test>
					<rul:GreaterThan>
						<rul:FirstValue Type="decimal" FromConstant="0" FromDataDef="@Premium" Order="DataDefInputParamConstant" />
						<rul:Constant Type="decimal">0</rul:Constant>
					</rul:GreaterThan>
				</rul:Test>
				<rul:Then>
					<rul:Sequence>
						<rul:Constant Type="integer" ToDataDef="@PremiumIndicator">1</rul:Constant>
					</rul:Sequence>
				</rul:Then>
				<rul:Else>
					<rul:Sequence>
						<rul:Constant Type="integer" ToDataDef="@PremiumIndicator">0</rul:Constant>
					</rul:Sequence>
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
	<rul:Rule Name="ErcProcess" Type="none" DataDefGroup="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthers">
		<rul:Sequence>
			<rul:RunRule Type="none" FileName="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthersRules" Rule="SetCoverageOnPolicyIndicator" ClearCache="true" />
			<rul:If>
				<rul:Test>
					<rul:Equal>
						<rul:FirstValue Type="integer" FromConstant="0" FromDataDef="@CoverageOnPolicyIndicator" Order="DataDefInputParamConstant" />
						<rul:Constant Type="integer">0</rul:Constant>
					</rul:Equal>
				</rul:Test>
				<rul:Then>
					<rul:Sequence>
						<rul:Constant Type="decimal" ToDataDef="@Premium">0.0</rul:Constant>
					</rul:Sequence>
				</rul:Then>
				<rul:Else>
					<rul:RunRule Type="none" FileName="CommercialPropertyStructureBuildersRiskBuildingMaterialsAndSuppliesOfOthersRules" Rule="ErcRate" ClearCache="true" />
				</rul:Else>
			</rul:If>
		</rul:Sequence>
	</rul:Rule>
</rul:Rules>